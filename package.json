    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
